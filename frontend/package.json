    "next": "^15.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.65.0",
    "socket.io-client": "^4.6.1",
    "tailwind-merge": "^2.0.0",
    "zod": "3.25.76",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.56.1",
